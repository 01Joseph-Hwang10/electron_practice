{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "main": "entry/dist/entry.js",
  "scripts": {
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "svelte-silent": "cross-env BROWSER=none DEBUG=true npm run svelte-build",
    "svelte-start": "sirv public --no-clear",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "electron-start": "electron .",
    "electron-dev": "npm run electron-start",
    "dev": "tsc ./entry/entry.ts --outDir ./entry/dist/ && ./dev-server/main.sh",
    "build": "npm run svelte-build && electron-builder",
    "release": "npm run svelte-build && electron-builder --publish=always"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^9",
    "bootstrap-icons": "^1.5.0",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electron-reloader": "^1.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^7",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.41.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "sirv-cli": "^1.0.0"
  }
}
