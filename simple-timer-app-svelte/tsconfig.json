{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/"
      ]
    },
    "esModuleInterop": false
  },
  "include": ["src/**/*", "entry/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*", "/dist/*", "/build/*"]
}